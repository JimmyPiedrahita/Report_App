<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/wall_bag">
        <shape android:shape="rectangle">
            <solid android:color="@color/Color_terciario_light"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>
    <item android:id="@+id/progress_bar">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <corners android:radius="50dp"/>
                <solid android:color="@color/Color_principal_dark"/>
            </shape>
        </scale>
    </item>
</layer-list>